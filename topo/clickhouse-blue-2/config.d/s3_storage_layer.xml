<clickhouse>
  <storage_configuration>
    <disks>
      <s3_disk>
        <type>s3</type>
        <endpoint>http://192.168.51.166:9090/clickhouse-data/s3_disk/</endpoint>
        <access_key_id>data</access_key_id>
        <secret_access_key>data@123</secret_access_key>
        <metadata_path>/var/lib/clickhouse/disks/s3_disk/</metadata_path>
        <cache_enabled>true</cache_enabled>
        <data_cache_enabled>true</data_cache_enabled>
        <cache_path>/var/lib/clickhouse/disks/s3_disk/cache/</cache_path>
      </s3_disk>
    </disks>
    <policies>
      <hot_cold>
        <volumes>
          <hot>
            <disk>default</disk>
          </hot>
          <cold>
            <disk>s3_disk</disk>
          </cold>
        </volumes>
        <move_factor>0.1</move_factor>
      </hot_cold>
      <!-- <s3_main>
        <volumes>
          <main>
            <disk>s3_disk</disk>
          </main>
        </volumes>
      </s3_main> -->
    </policies>
  </storage_configuration>
</clickhouse>
